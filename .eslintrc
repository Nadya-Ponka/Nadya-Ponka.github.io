{
  parser: "babel-eslint",
  "rules": {
      "linebreak-style": 0,
      "indent": ["error", 4],
      "no-alert": 0,
      "prefer-destructuring": ["error", {"object": false, "array": false}],
      "no-use-before-define":  ["error", { "functions": false }],
      "no-param-reassign": 0
  },
  "extends": ["airbnb-base"],
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "document": false
  },
  
}
